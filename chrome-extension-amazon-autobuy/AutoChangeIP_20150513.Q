[Script]
Form1.Show 
Form1.Label1.Caption = ""
Form1.Label2.Caption = ""
Form1.Label3.Caption = ""
Form1.Label4.Caption = ""
Form1.Label5.Caption = ""
Form1.Label6.Caption = ""
Form1.Label7.Caption = ""
While 1
	winx = 2000
	winy = 1200
	FindPic 0, 0, winx, winy, "Attachment:\autobuy.bmp", 0.9, autoICOX, autoICOY
	
	Form1.Label1.Caption = Now & " 检测中..."
	Form1.Label2.Caption = ""
	Form1.Label3.Caption = ""
	Form1.Label4.Caption = ""
	Form1.Label5.Caption = ""
	Form1.Label6.Caption = ""
	Form1.Label7.Caption = ""

	//
//Form1.ListBox1.InsertItem Date
	
	//如果出现提示
	If autoICOX > 0 And autoICOY > 0 Then 
		
		Form1.Label1.Caption = Now & " 检测到通知"
		Form1.Label2.Caption  = autoICOX
		Form1.Label3.Caption = autoICOY
		
		Form1.Label4.Caption = Now & " 检测代理按钮..."
		// 寻找改变ip按钮
		FindPic 0, 0, winx, winy, "Attachment:\btn-change.bmp", 0.9, btnChangeX, btnChangeY
		

		If btnChangeX > 0 And btnChangeY > 0 Then 
		
		
			Form1.Label4.Caption = Now & " 检测到代理按钮"
			Form1.Label5.Caption  = btnChangeX
			Form1.Label6.Caption = btnChangeY
		
			MoveTo btnChangeX + 77, btnChangeY + 30
			LeftClick 1
			
			Delay 10000
			
			
			Form1.Label7.Caption = "等待代理状态"
			// 等待状态更新为已连接
			FindPic 0, 0, winx, winy, "Attachment:\status-connect.bmp", 0.9, statusX, statusY
		
			While statusX <= 0 And ststusY <= 0
    			FindPic 0, 0, winx, winy, "Attachment:\status-connect.bmp", 0.9, statusX, statusY
			Wend
			
			Delay 3000
			Form1.Label7.Caption = "代理完成修改IP，30秒后点击通知"
			
			
			Delay 30000
			// 点击通知 以重新开始 ，点击两次，以保证被点击了
			MoveTo autoICOX + 50, autoICOY + 50
			LeftClick 1
				
			Form1.Label7.Caption = "为保证点击，30秒后再次点击通知"
			Delay 30000
			
			MoveTo autoICOX + 50, autoICOY + 50
			LeftClick 1
			
		End If
	
	
	//当循环条件成立的时候，反复执行循环体
	End If
	Delay 1000
Wend
